{
    "name": "itega/laracsv-utf8",
    "description": "A fork of usmanhalalit's Laravel package to easily generate CSV files from Eloquent model.",
    "homepage": "https://github.com/itega/laracsv",
    "keywords": ["laravel", "csv", "eloquent", "export"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors":
    [
        {
            "name": "Julien ZOLLI",
            "email": "julien.zolli@viacesi.fr",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=5.6.4",
        "illuminate/database": ">=5.2",
        "league/csv": ">=8.2"
    },

    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "5.7.*"
    },

    "autoload":	{
        "psr-4": {
            "Laracsv\\": "src/Laracsv/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Laracsv\\": "tests/Laracsv/"
        },

        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
